{
  "name": "@account.tech/dao",
  "author": "Thouny <thouny@tuta.io>",
  "description": "DAO Account SDK powered by account.tech",
  "version": "0.1.23",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/esm/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "build": "rm -rf dist/ && rm -rf tsconfig.tsbuildinfo && bun build:tsup && bun build:types",
    "build:tsup": "tsup ./src/* --format esm,cjs --sourcemap",
    "build:types": "tsc --build",
    "codegen": "sui-ts-codegen generate"
  },
  "dependencies": {
    "@account.tech/core": "0.1.30",
    "@mysten/sui": "1.42.0",
    "bun-types": "^1.3.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.19.24",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
